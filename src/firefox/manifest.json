{
  "manifest_version": 2,
  "name": "POC Automator",
  "version": "1.0.0",
  "description": "Extract contact information from company profile tables including Founders & Key People and Senior Management sections",
  
  "browser_specific_settings": {
    "gecko": {
      "id": "pocautomator@yourorganization.com",
      "strict_min_version": "55.0"
    }
  },
  
  "permissions": [
    "activeTab",
    "tabs"
  ],
  
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "POC Automator"
  },
  
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}